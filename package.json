{
  "name": "automaton",
  "productName": "Automaton",
  "version": "1.6.4",
  "description": "automation tool for Tibia",
  "type": "module",
  "main": "electron/main.js",
  "extraResources": [
    "src/preload.js"
  ],
  "scripts": {
    "start": "webpack --mode development && electron .",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "package": "npm run build && electron-builder ",
    "make": "npm run build && electron-builder ",
    "publish": "npm run build && electron-builder -p always",
    "lint": "eslint . --ext .js,.jsx,.mjs"
  },
  "build": {
    "appId": "com.feiron.automaton",
    "productName": "Automaton",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "src/**/*",
      "package.json"
    ],
    "extraResources": [
      "src/preload.js"
    ],
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Utility"
    },
    "asar": true,
    "publish": null
  },
  "keywords": [],
  "author": "feiron <pawel.rogowski.praca@gmail.com>",
  "license": "CC-BY-ND-4.0",
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.1",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "lodash": "^4.17.21",
    "modern-normalize": "^2.0.0",
    "node-loader": "^2.0.0",
    "path-browserify": "^1.0.1",
    "play-sound": "^1.1.6",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.21.1",
    "redux-logger": "^3.0.6",
    "screenshot-desktop": "^1.15.0",
    "styled-components": "^6.1.6",
    "webpack-node-externals": "^3.0.0",
    "x11": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^29.1.5",
    "eslint": "^8.56.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.6.0",
    "javascript-obfuscator": "^4.1.0",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-obfuscator": "^3.5.1"
  }
}
