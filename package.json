{
  "name": "bright_ideal_grid",
  "version": "1.3.5b",
  "description": "automation tool for Tibia",
  "type": "module",
  "main": "electron/main.js",
  "extraResources": [
    "frontend/preload.js"
  ],
  "scripts": {
    "pre-build": "node scripts/set-random-name.js",
    "start": "webpack --mode production && electron .",
    "rebuild": "electron-rebuild",
    "build": "cross-env NODE_ENV=production webpack --mode production",
    "package": "npm run build && electron-builder ",
    "obfuscate": "node scripts/obfuscate-for-build.js",
    "make": "rm -rf dist && npm run pre-build && npm run build && electron-builder",
    "lint": "eslint . --ext .js,.jsx,.mjs",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.electron.bright_ideal_grid",
    "productName": "Bright Ideal Grid",
    "directories": {
      "output": "dist",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "frontend/**/*",
      "package.json",
      "!scripts/*"
    ],
    "extraFiles": [
      {
        "from": ".electron-obfuscated",
        "to": "electron",
        "filter": [
          "**/*",
          "!path-map.json"
        ]
      }
    ],
    "extraResources": [
      "frontend/preload.js",
      {
        "from": "resources",
        "to": "resources",
        "filter": [
          "**/*"
        ]
      }
    ],
    "linux": {
      "target": [
        "AppImage"
      ],
      "icon": "electron/icons/automaton.png",
      "asarUnpack": "**/*.node",
      "category": "Utility"
    },
    "asar": true,
    "publish": null
  },
  "keywords": [],
  "author": "feiron <pawel.rogowski.praca@gmail.com>",
  "license": "CC-BY-ND-4.0",
  "dependencies": {
    "@monaco-editor/react": "^4.7.0",
    "@reduxjs/toolkit": "^2.0.1",
    "@textea/json-viewer": "^4.0.1",
    "ahocorasick": "^1.0.2",
    "compression-webpack-plugin": "^11.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "ioredis": "^5.6.0",
    "konva": "^9.3.20",
    "lodash": "^4.17.21",
    "modern-normalize": "^2.0.0",
    "monaco-editor": "^0.52.2",
    "node-addon-api": "^8.4.0",
    "node-loader": "^2.0.0",
    "node-valkey": "^1.0.3",
    "path-browserify": "^1.0.1",
    "play-sound": "^1.1.6",
    "pngjs": "^7.0.0",
    "process": "^0.11.10",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-feather": "^2.0.10",
    "react-konva": "^18.2.10",
    "react-redux": "^9.0.4",
    "react-router-dom": "^6.28.0",
    "react-table": "^7.8.0",
    "redis": "^4.7.0",
    "redis-memory-server": "^0.12.1",
    "redux-logger": "^3.0.6",
    "sharp": "^0.34.2",
    "shm-typed-array": "^0.1.1",
    "styled-components": "^6.1.6",
    "use-image": "^1.1.4",
    "uuid": "^11.0.3",
    "wasmoon": "^1.16.0",
    "webpack-node-externals": "^3.0.0",
    "keypress-native": "file:./nativeModules/keypress",
    "minimap_matcher-native": "file:./nativeModules/minimapMatcher",
    "windowinfo-native": "file:./nativeModules/windowInfo",
    "pathfinder-native": "file:./nativeModules/pathfinder",
    "x11-region-capture-native": "file:./nativeModules/x11RegionCapture",
    "find-sequences-native": "file:./nativeModules/findSequences",
    "font-ocr": "file:./nativeModules/fontOcr"
  },
  "devDependencies": {
    "@babel/core": "^7.23.6",
    "@babel/eslint-parser": "^7.23.3",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "babel-loader": "^9.1.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "electron": "^29.1.5",
    "electron-builder": "^25.1.8",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.56.0",
    "file-loader": "^6.2.0",
    "fs-extra": "^11.3.0",
    "html-webpack-plugin": "^5.6.0",
    "monaco-editor-webpack-plugin": "^7.1.0",
    "prettier": "^3.1.1",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.10",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
